<Window x:Class="ColoritWPF.AddCarModelDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Добавить модель автомобиля:" Height="175" Width="300">
    <Grid>
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,48,0,0" Name="txtbx_carModel" VerticalAlignment="Top" Width="254" />
        <Label Content="Введите модель авто:" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <Button Content="Отмена" Height="23" HorizontalAlignment="Left" Margin="12,91,0,0" Name="btn_Cancel" VerticalAlignment="Top" Width="75" Click="btn_Cancel_Click" />
        <Button Content="Добавить" Height="23" HorizontalAlignment="Left" Margin="191,91,0,0" Name="btn_Add" VerticalAlignment="Top" Width="75" Click="btn_Add_Click" />
    </Grid>
</Window>
